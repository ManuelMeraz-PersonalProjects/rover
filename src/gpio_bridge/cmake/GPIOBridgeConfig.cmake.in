# - Config file for the gpio_bridge package
# It defines the following variables
#  GPIOBRIDGE_INCLUDE_DIRS - include directories for gpio_bridge
#  GPIOBRIDGE_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(GPIOBRIDGE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(GPIOBRIDGE_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET gpio_bridge)
    include("${GPIOBRIDGE_CMAKE_DIR}/GPIOBRidgeTargets.cmake")
endif()

# These are IMPORTED targets created by GPIOBridgeTargets.cmake
set(GPIOBRIDGE_LIBRARIES gpio_bridge)
