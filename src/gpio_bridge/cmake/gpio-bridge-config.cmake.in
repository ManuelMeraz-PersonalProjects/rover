# - Config file for the gpio_bridge package
# It defines the following variables
#  GPIO_BRIDGE_INCLUDE_DIRS - include directories for gpio_bridge
#  GPIO_BRIDGE_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(GPIO_BRIDGE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(GPIO_BRIDGE_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET gpio_bridge)
    include("${GPIO_BRIDGE_CMAKE_DIR}/gpio-bridge-targets.cmake")
endif()

# These are IMPORTED targets created by gpio-bridge-targets.cmake
set(GPIO_BRIDGE_LIBRARIES gpio_bridge)
